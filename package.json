{
  "name": "solid-toast",
  "version": "0.3.1",
  "description": "Customizable Toast Notifications for SolidJS",
  "source": "./src/index.tsx",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ardeora/solid-toast.git"
  },
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "solidjs",
    "notifications",
    "toast"
  ],
  "author": "Aryan Deora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ardeora/solid-toast/issues"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "minify": "terser --compress --mangle --module -- dist/cjs/index.js > dist/cjs/index.min.js",
    "format": "prettier --write src dev"
  },
  "homepage": "https://github.com/ardeora/solid-toast#readme",
  "dependencies": {},
  "peerDependencies": {
    "solid-js": "^1.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.1",
    "@types/node": "^17.0.35",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-preset-solid": "^1.4.0",
    "solid-js": "^1.4.2",
    "terser": "^5.14.1",
    "typescript": "^4.6.4"
  }
}
